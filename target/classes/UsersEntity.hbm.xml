<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.Hibernate.UsersEntity" table="users" schema="java_web">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="userName">
            <column name="userName" sql-type="text" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="text" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>